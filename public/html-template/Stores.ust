<div style="background:white" class="row small-up-1 medium-up-2 large-up-4" id="tile-continer">
    <% _.each(storeCollection, function(i) { %>
        <div class="column">
            <br>
            <img src="<%= i.picture %>" class="thumbnail" alt=""/>
            <p><%= i.title %></p>
        </div>
    <% }); %>
</div>